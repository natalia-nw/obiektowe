public class Main {

    public static void main(String[] args) {
        //zad1
        java.lang.System.out.println(31 + 29 + 31 + "\n");

        //zad2
        int suma = 0;
        for (int i = 0; i <= 10; i++) {
            suma += i;
        }
        java.lang.System.out.println(suma + "\n");

        //zad3
        int iloczyn = 1;
        for (int i = 1; i <= 10; i++) {
            iloczyn *= i;
        }
        java.lang.System.out.println(iloczyn + "\n");

        //zad4
        int saldo = 1000;
        for (int i = 0; i < 4; i++) {
            saldo *= 1.06;
        }
        java.lang.System.out.println(saldo + "\n");

        //zad5
        java.lang.System.out.println("______\n" + "|Java|\n" + "------\n");

